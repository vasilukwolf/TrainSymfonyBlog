{% extends "::base.html.twig" %}
{% block body %}
	<h1 class="page_title" id="page_title">
	{{blog.title}}
	</h1>
	<article>
		<div class="container blog-container">
			<div class="field field-body">
				{{blog.body|raw}}
			</div>
			<div class="field field-crated">
				{{blog.created|date("d-m-Y")}}
			</div>
		</div>
	</article>
{% endblock %}